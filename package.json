{
  "name": "github-takeout",
  "version": "0.0.5",
  "description": "A utility for cloning all your repos, including issues, discussions, stargazers and more!",
  "bin": "lib/cjs/index.js",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "esnext": "lib/esnext/index.js",
  "typings": "lib/esm/index.d.ts",
  "repository": "https://github.com/lukasbach/github-takeout",
  "author": "Lukas Bach <npm@lukasbach.com>",
  "license": "MIT",
  "files": [
    "lib/**/*",
    "!lib/**/*.spec.js",
    "demo.gif"
  ],
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "run-s \"build:*\"",
    "build:cjs": "tsc --outDir lib/cjs",
    "build:esm": "tsc -m es2015 --outDir lib/esm",
    "build:esnext": "tsc -m esnext --outDir lib/esnext",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --fix",
    "lint:test": "eslint .",
    "prepublishOnly": "yarn build",
    "clean": "rimraf ./lib/ ./exec/"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/marked": "^5.0.2",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.12.12",
    "@types/node-fetch": "^2.6.11",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^9.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "@octokit/rest": "^20.1.1",
    "@octokit/types": "^13.5.0",
    "fs-extra": "^11.2.0",
    "marked": "^12.0.2",
    "mustache": "^4.2.0",
    "node-fetch": "^3.3.2",
    "prompts": "^2.4.2",
    "sanitize-html": "^2.13.0",
    "simple-git": "^3.24.0",
    "zip-a-folder": "^3.1.6"
  }
}
