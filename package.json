{
  "name": "github-takeout",
  "version": "0.0.1",
  "description": "A utility for cloning all your repos, including issues, discussions, stargazers and more!",
  "bin": "lib/cjs/index.js",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "esnext": "lib/esnext/index.js",
  "typings": "lib/esm/index.d.ts",
  "repository": "https://github.com/lukasbach/github-takeout",
  "author": "Lukas Bach <npm@lukasbach.com>",
  "license": "MIT",
  "files": [
    "lib/**/*",
    "!lib/**/*.spec.js"
  ],
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "run-s \"build:*\"",
    "build:cjs": "tsc --outDir lib/cjs",
    "build:esm": "tsc -m es2015 --outDir lib/esm",
    "build:esnext": "tsc -m esnext --outDir lib/esnext",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --fix",
    "lint:test": "eslint .",
    "prepublishOnly": "yarn build",
    "clean": "rimraf ./lib/ ./exec/"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "@octokit/rest": "^19.0.5",
    "@octokit/types": "^8.0.0",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "prompts": "^2.4.2",
    "simple-git": "^3.14.1",
    "zip-a-folder": "^1.1.5"
  }
}
